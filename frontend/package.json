{
  "name": "frontend-submodule",
  "main": "src/index.ts",
  "scripts": {
    "all": "npm run assemble && npm run check",
    "clean": "rm -rf dist",
    "check": "npm test",
    "test": "cross-env TS_NODE_FILES=true nyc jest",
    "assemble": "webpack --mode=production"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": "src",
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "nyc": "^15.1.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  }
}
